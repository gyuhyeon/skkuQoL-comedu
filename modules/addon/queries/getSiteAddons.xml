<query id="getSiteAddons" action="select">
    <tables>
      <table name="addons_site" />
    </tables>
    <conditions>
      <condition operation="equal" column="site_srl" var="site_srl" notnull="notnull" />
    </conditions>
    <navigation>
      <index var="list_order" default="addon" order="asc" />
    </navigation>
</query>
