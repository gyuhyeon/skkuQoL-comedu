<query id="getDefaultGroup" action="select">
    <tables>
        <table name="member_group" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="is_default" default="Y" />
        <condition operation="equal" column="site_srl" var="site_srl" pipe="and" />
    </conditions>
</query>
