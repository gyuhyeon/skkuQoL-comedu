<query id="getMemberInfoByEmailAddress" action="select">
    <tables>
        <table name="member" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="lcase(email_address)" var="email_address" notnull="notnull" />
    </conditions>
</query>
