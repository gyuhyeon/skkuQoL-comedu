<query id="getInstalledPackage" action="select">
    <tables>
        <table name="ai_installed_packages" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="package_srl" var="package_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
