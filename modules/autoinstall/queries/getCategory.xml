        <table name="ai_remote_categories" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="category_srl" var="category_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
