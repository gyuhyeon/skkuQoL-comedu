<query id="getPackageCount" action="select">
    <tables>
        <table name="autoinstall_packages" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="category_srl" var="category_srl" filter="number" />
    </conditions>
</query>
