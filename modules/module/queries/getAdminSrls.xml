<query id="getAdminSrls" action="select">
    <tables>
        <table name="member" />
    </tables>
    <columns>
        <column name="member_srl" />
    </columns>
    <conditions>
        <condition operation="in" column="user_id" var="user_ids" />
        <condition operation="in" column="email_address" var="email_address" />
    </conditions>
</query>
