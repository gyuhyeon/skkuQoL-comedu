        <table name="module_grants" />
    </tables>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" notnull="notnull" filter="number" />
    </conditions>
    <navigation>
        <index var="sort_index" default="group_srl" order="asc" />
    </navigation>
</query>
