<filter name="update_category" module="module" act="procModuleAdminUpdateCategory">
    <form>
        <node target="module_category_srl" required="true" />
        <node target="mode" required="true" />
    </form>
    <response callback_func="completeUpdateCategory">
        <tag name="error" />
        <tag name="message" />
    </response>
</filter>
