        <table name="menu_item" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="menu_item_srl" var="menu_item_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
