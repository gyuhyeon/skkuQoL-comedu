<query id="updateMenu" action="update">
    <tables>
        <table name="menu" />
    </tables>
    <columns>
        <column name="title" var="title" notnull="notnull" />
    </columns>
    <conditions>
        <condition operation="equal" column="menu_srl" var="menu_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
