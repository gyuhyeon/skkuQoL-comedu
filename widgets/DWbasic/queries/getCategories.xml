<query id="getCategories" action="select">
    <tables>
        <table name="document_categories" />
    </tables>
    <columns>
        <column name="module_srl" />
        <column name="category_srl" />
        <column name="title" />
    </columns>
    <conditions>
        <condition operation="in" column="module_srl" var="module_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
